{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@/lib": "./lib",
          "@/utils": "./utils",
          "@/public": "./public",
          "@/components": "./components"
        }
      }
    ]
  ]
}
